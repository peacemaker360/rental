        "isXenon": false,
        "hyperV": false,
        "targetWorkerCount": 0,
        "targetWorkerSizeId": 0,
        "zoneRedundant": false
      }
    },
    {
      "type": "Microsoft.DBforMySQL/flexibleServers/databases",
      "apiVersion": "2021-12-01-preview",
      "name": "[concat(parameters('flexibleServers_dbserver_name'), '/app')]",
      "dependsOn": [
        "[resourceId('Microsoft.DBforMySQL/flexibleServers', parameters('flexibleServers_dbserver_name'))]"
      ],
      "properties": {
        "charset": "utf8",
        "collation": "utf8_general_ci"
      }
    },
    {
      "type": "Microsoft.DBforMySQL/flexibleServers/databases",
      "apiVersion": "2021-12-01-preview",
      "name": "[concat(parameters('flexibleServers_dbserver_name'), '/information_schema')]",
      "dependsOn": [
        "[resourceId('Microsoft.DBforMySQL/flexibleServers', parameters('flexibleServers_dbserver_name'))]"
      ],
      "properties": {
        "charset": "utf8",
        "collation": "utf8_general_ci"
      }
    },
    {
      "type": "Microsoft.DBforMySQL/flexibleServers/databases",
      "apiVersion": "2021-12-01-preview",
      "name": "[concat(parameters('flexibleServers_dbserver_name'), '/mysql')]",
      "dependsOn": [
        "[resourceId('Microsoft.DBforMySQL/flexibleServers', parameters('flexibleServers_dbserver_name'))]"
      ],
      "properties": {
        "charset": "latin1",
        "collation": "latin1_swedish_ci"
      }
    },
    {
      "type": "Microsoft.DBforMySQL/flexibleServers/databases",
      "apiVersion": "2021-12-01-preview",
      "name": "[concat(parameters('flexibleServers_dbserver_name'), '/performance_schema')]",
      "dependsOn": [
        "[resourceId('Microsoft.DBforMySQL/flexibleServers', parameters('flexibleServers_dbserver_name'))]"
      ],
      "properties": {
        "charset": "utf8",
        "collation": "utf8_general_ci"
      }
    },
    {
      "type": "Microsoft.DBforMySQL/flexibleServers/databases",
      "apiVersion": "2021-12-01-preview",
      "name": "[concat(parameters('flexibleServers_dbserver_name'), '/sys')]",
      "dependsOn": [
        "[resourceId('Microsoft.DBforMySQL/flexibleServers', parameters('flexibleServers_dbserver_name'))]"
      ],
      "properties": {
        "charset": "utf8",
        "collation": "utf8_general_ci"
      }
    },
    {
      "type": "Microsoft.DBforMySQL/flexibleServers/firewallRules",
      "apiVersion": "2021-12-01-preview",
      "name": "[concat(parameters('flexibleServers_dbserver_name'), '/AllowAllAzureServicesAndResourcesWithinAzureIps_2023-5-4_18-26-14')]",
      "dependsOn": [
        "[resourceId('Microsoft.DBforMySQL/flexibleServers', parameters('flexibleServers_dbserver_name'))]"
      ],
      "properties": {
        "startIpAddress": "0.0.0.0",
        "endIpAddress": "0.0.0.0"
      }
    },
    {
      "type": "Microsoft.Web/sites",
      "apiVersion": "2022-09-01",
      "name": "[parameters('sites_name')]",
      "location": "Switzerland North",
      "dependsOn": [
        "[resourceId('Microsoft.Web/serverfarms', parameters('serverfarms_ASP_name'))]"
