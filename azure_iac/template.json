    "sites_name": {
      "defaultValue": "mgwrent",
      "type": "string"
    },
    "serverfarms_ASP_name": {
      "defaultValue": "ASP-rgmgwrental-bce7",
      "type": "string"
    },
    "flexibleServers_dbserver_name": {
      "defaultValue": "mgwrent-dbserver",
      "type": "string"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.DBforMySQL/flexibleServers",
      "apiVersion": "2022-09-30-preview",
      "name": "[parameters('flexibleServers_dbserver_name')]",
      "location": "Switzerland North",
      "sku": {
        "name": "Standard_B1s",
        "tier": "Burstable"
      },
      "properties": {
        "administratorLogin": "dbaadmin",
        "storage": {
          "storageSizeGB": 20,
          "iops": 360,
          "autoGrow": "Enabled",
          "autoIoScaling": "Disabled",
          "logOnDisk": "Disabled"
        },
        "version": "5.7",
        "availabilityZone": "3",
        "maintenanceWindow": {
          "customWindow": "Disabled",
          "dayOfWeek": 0,
          "startHour": 0,
          "startMinute": 0
        },
        "replicationRole": "None",
        "network": {},
        "backup": {
          "backupRetentionDays": 7,
          "geoRedundantBackup": "Disabled"
        },
        "highAvailability": {
          "mode": "Disabled"
        }
      }
    },
    {
      "type": "Microsoft.Web/serverfarms",
      "apiVersion": "2022-09-01",
      "name": "[parameters('serverfarms_ASP_name')]",
      "location": "Switzerland North",
      "sku": {
        "name": "B1",
        "tier": "Basic",
        "size": "B1",
        "family": "B",
        "capacity": 1
      },
      "kind": "linux",
      "properties": {
        "perSiteScaling": false,
        "elasticScaleEnabled": false,
        "maximumElasticWorkerCount": 1,
        "isSpot": false,
        "freeOfferExpirationTime": "2023-11-03T19:53:56.3866667",
